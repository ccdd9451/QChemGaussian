Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"Predefined", " ", "constants"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"c", " ", ":=", " ", 
     RowBox[{"Quantity", "[", "\"\<SpeedOfLight\>\"", "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"avo", " ", ":=", " ", 
     RowBox[{"Quantity", "[", "\"\<AvogadroConstant\>\"", "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "SI", " ", "Unit", " ", "known", " ", "parameters", " ", "for", " ", 
     "HCl"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Omega]", ":=", 
     RowBox[{"Quantity", "[", 
      RowBox[{"2866", ",", " ", 
       RowBox[{"1", "/", "\"\<Centimeters\>\""}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"\[Mu]", ":=", 
    RowBox[{
     RowBox[{"Quantity", "[", 
      RowBox[{
       FractionBox[
        RowBox[{"35", "\[Times]", "1"}], 
        RowBox[{"35", "+", "1"}]], ",", " ", 
       FractionBox["\"\<Grams\>\"", "\"\<Moles\>\""]}], "]"}], " ", 
     SuperscriptBox["avo", 
      RowBox[{"-", "1"}]]}]}], "\[IndentingNewLine]", 
   RowBox[{"k", ":=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"2", "\[Pi]", " ", "\[Omega]", " ", "c"}], ")"}], "2"], " ", 
     "\[Mu]"}]}], "\[IndentingNewLine]", 
   RowBox[{"d", ":=", 
    RowBox[{
     RowBox[{"Quantity", "[", 
      RowBox[{"440.2", ",", 
       FractionBox["\"\<Kilojoules\>\"", "\"\<Moles\>\""]}], "]"}], " ", 
     SuperscriptBox["avo", 
      RowBox[{"-", "1"}]]}]}]}]}]], "Input",
 CellChangeTimes->{{3.7195864334692097`*^9, 3.7195866749033546`*^9}, {
  3.7195867084023647`*^9, 3.719586733743169*^9}, {3.7195867687755127`*^9, 
